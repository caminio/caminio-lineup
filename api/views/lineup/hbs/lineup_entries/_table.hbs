<table class="caminio entries-table box fill-box">
  <thead>
    <tr>
      <th class="item-id">#</th>
      <th>{{t 'entry.title'}}</th>
      <th>{{t 'entry.date'}}</th>
      <th>{{t 'entry.venues'}}</th>
      <th>{{t 'updated_at'}}</th>
      <th>{{t 'updated_by'}}</th>
      <th class="short-info"></th>
    </tr>
  </thead>
  <tbody>
    {{#each model itemController='lineup_entry'}}
      <tr {{action 'showEntry'}}>
        <td class="item-id">{{content.entryId}}</td>
        <td>{{content.curTranslation.title}}</td>
        <td>{{content.period}}</td>
        <td>{{content.venues}}</td>
        <td>{{formatDate content.updatedAt}}</td>
        <td>{{content.updatedBy.name}}</td>
        <td class="short-info">
          {{content.curTranslation.title}}
          <br>
          {{content.period}}
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>