<section class="plain-wrapper">

  <header>
    <div class="box title-box website">
      <div class="main-ctrl">
        <button class="btn primary icon-text" {{action 'newItem'}}>
          <i class="fa fa-plus"></i>
          {{t 'entry.create'}}
        </button>
      </div>
      <h1>{{t 'entries.title'}}</h1>

    </div>

  </header>

  <div class="row span-row">

    <div class="col-sidebar">
      
      {{partial 'lineup_entries/search'}}

      <div class="box fill-box margin-top">
        {{partial 'lineup/labels'}}
      </div>

    </div>

    <div class="col-table animate-width" id="entries-table">
      {{outlet 'table'}}
    </div>

  </div>

</section>
